<CascadingAuthenticationState>
    <Router AppAssembly="@typeof(Program).Assembly" AdditionalAssemblies="new[] { typeof(Portal).Assembly}" PreferExactMatches="@true">
        <Found Context="routeData">
            <KenovaRouteView RouteData="@routeData" />
        </Found>
        <NotFound>
            <KenovaRouteNotFound />
        </NotFound>
    </Router>
</CascadingAuthenticationState>

