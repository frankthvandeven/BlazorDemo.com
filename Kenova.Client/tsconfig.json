// https://www.typescriptlang.org/docs/handbook/tsconfig-json.html

{
  "compilerOptions": {
    "module": "system",
    "strict": false,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "noEmitOnError": false,
    "removeComments": true,
    "sourceMap": false,
    "outFile": "wwwroot/kenova.js",
    "target": "es6"
  },
  "include": [ "**/*.ts" ],
  "exclude": [ "wwwroot" ]
}


